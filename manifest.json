{
  "manifest_version": 3,
  "name": "Velog Stats Tracker",
  "version": "1.6",
  "description": "Velog 블로그의 총 조회수 및 통계를 제공합니다.",
  "permissions": ["storage"],
  "host_permissions": ["https://v2.velog.io/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icon.png"
  }
}
